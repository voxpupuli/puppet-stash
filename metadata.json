{
  "name": "puppet-stash",
  "version": "2.0.0",
  "author": "puppet",
  "summary": "Install Atlassian stash",
  "license": "MIT",
  "source": "https://github.com/voxpupuli/puppet-stash.git",
  "project_page": "https://github.com/voxpupuli/puppet-stash",
  "issues_url": "https://github.com/voxpupuli/puppet-stash/issues",
  "description": "Module to install Atlassian Stash. On-premises source code management for Git that's secure, fast, and enterprise grade.",
  "tags": ["bitbucket", "stash"],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "12.04",
        "14.04"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "7"
      ]
    }
  ],
  "dependencies": [
    {
      "name": "puppet/archive",
      "version_requirement": ">= 0.4.4"
    },
    {
      "name": "puppetlabs/inifile",
      "version_requirement": ">= 1.0.0"
    },
    {
      "name": "puppet/staging",
      "version_requirement": ">= 1.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.0.0"
    }
  ],
  "requirements": [
    {
      "name": "pe",
      "version_requirement": "3.x"
    },
    {
      "name": "puppet",
      "version_requirement": ">=3.4.0 <4.0.0"
    }
  ]
}
